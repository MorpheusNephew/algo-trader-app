{
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc -p ./tsconfig.json",
    "cleanDir": "rimraf ../nodejs",
    "babel": "babel . --out-dir ../nodejs --extensions '.ts' -D",
    "prebuild": "yarn && yarn cleanDir && yarn tsc",
    "build": "yarn babel && cd ../nodejs && yarn install --prod",
    "postbuild": "rimraf ../nodejs/babel.config.json && rimraf ../nodejs/tsconfig.json"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-typescript": "^7.15.0",
    "@types/uuid": "^8.3.1",
    "uuid": "^8.3.2"
  }
}
